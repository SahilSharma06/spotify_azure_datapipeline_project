{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifyazure-datafactory"
		},
		"AzureDataLakeStorage_LinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_LinkedService'"
		},
		"AzureSQL_LinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSQL_LinkedService'"
		},
		"AzureDataLakeStorage_LinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageazureacc.dfs.core.windows.net/"
		},
		"AzureSQL_LinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifysqlserverazure.database.windows.net"
		},
		"AzureSQL_LinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AzureSQL_spotify_database"
		},
		"AzureSQL_LinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsahil"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifyazure-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_LinkedService_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_LinkedService_accountKey')]"
					},
					"AzureSQL_LinkedService_password": {
						"value": "[parameters('AzureSQL_LinkedService_password')]"
					},
					"AzureDataLakeStorage_LinkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_LinkedService_properties_typeProperties_url')]"
					},
					"AzureSQL_LinkedService_properties_typeProperties_server": {
						"value": "[parameters('AzureSQL_LinkedService_properties_typeProperties_server')]"
					},
					"AzureSQL_LinkedService_properties_typeProperties_database": {
						"value": "[parameters('AzureSQL_LinkedService_properties_typeProperties_database')]"
					},
					"AzureSQL_LinkedService_properties_typeProperties_userName": {
						"value": "[parameters('AzureSQL_LinkedService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}